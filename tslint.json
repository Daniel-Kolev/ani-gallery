{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint-config-prettier", "tslint-react", "tslint-config-airbnb"],
  "rules": {
    "prettier": true,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "import-name": false,
    "no-boolean-literal-compare": false
  }
}
